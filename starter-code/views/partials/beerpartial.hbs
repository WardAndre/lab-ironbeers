<div class="col mb-4">
    <div class="card">
        <img src="{{this.image_url}}" class="card-img-top" alt="{{this.id}}">
        <div class="card-body">
            <h5 class="card-title"> <a href="/beer/{{this.id}}">{{this.name}}</a></h5>
            <h6 class="card-subtitle text-muted">{{this.tagline}}</h6>
            <p class="card-text">{{this.description}}</p>
        </div>
    </div>
</div>